{
  "canisters": {
    "px1m_backend": {
      "main": "src/px1m_backend/main.mo",
      "type": "motoko"
    },
    "px1m_frontend": {
      "dependencies": [
        "px1m_backend"
      ],
      "source": [
        "src/px1m_frontend/dist"
      ],
      "type": "assets",
      "workspace": "px1m_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8080",
      "replica": {
        "subnet_type": "system"
      },
      "type": "ephemeral"
    }
  },
  "output_env_file": ".env",
  "version": 1
}